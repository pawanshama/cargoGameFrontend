{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://e780-2402-e280-230d-3ff-f9e1-6449-ba90-46df.ngrok-free.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/tonconnect-manifest-v2.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/(.*)\\.(html|js|css|json|webp|woff2|woff|otf|ttf|png|jpg|jpeg|svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}
